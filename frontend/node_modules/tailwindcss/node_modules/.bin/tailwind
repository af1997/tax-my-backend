#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/ashle/Documents/Github/tax_my_backend/tax_my_backend/node_modules/.pnpm/tailwindcss@3.4.17_ts-node@_3cc8f304cb2ee16ff58a43319642471f/node_modules/tailwindcss/lib/node_modules:/mnt/c/Users/ashle/Documents/Github/tax_my_backend/tax_my_backend/node_modules/.pnpm/tailwindcss@3.4.17_ts-node@_3cc8f304cb2ee16ff58a43319642471f/node_modules/tailwindcss/node_modules:/mnt/c/Users/ashle/Documents/Github/tax_my_backend/tax_my_backend/node_modules/.pnpm/tailwindcss@3.4.17_ts-node@_3cc8f304cb2ee16ff58a43319642471f/node_modules:/mnt/c/Users/ashle/Documents/Github/tax_my_backend/tax_my_backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/ashle/Documents/Github/tax_my_backend/tax_my_backend/node_modules/.pnpm/tailwindcss@3.4.17_ts-node@_3cc8f304cb2ee16ff58a43319642471f/node_modules/tailwindcss/lib/node_modules:/mnt/c/Users/ashle/Documents/Github/tax_my_backend/tax_my_backend/node_modules/.pnpm/tailwindcss@3.4.17_ts-node@_3cc8f304cb2ee16ff58a43319642471f/node_modules/tailwindcss/node_modules:/mnt/c/Users/ashle/Documents/Github/tax_my_backend/tax_my_backend/node_modules/.pnpm/tailwindcss@3.4.17_ts-node@_3cc8f304cb2ee16ff58a43319642471f/node_modules:/mnt/c/Users/ashle/Documents/Github/tax_my_backend/tax_my_backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli.js" "$@"
else
  exec node  "$basedir/../../lib/cli.js" "$@"
fi
